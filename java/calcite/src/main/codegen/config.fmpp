      # List of additional join types. Each is a method with no arguments.
      # Example: LeftSemiJoin()
      joinTypes: [
      ]

      # List of methods for parsing custom SQL statements.
      # Note that DROP commands are handled specially by SqlCustomDrop
      # and SHOW commands are handled specially by SqlCustomShow because
      # all of the statements listed here have LOOKAHEAD(2) which is
      # hardcoded in Calcite's Parser.jj source code.
      statementParserMethods: [
        "SqlAlterDatabase(span())"
        "SqlAlterTable(span())"
        "SqlAlterServer(span())"
        "SqlAlterForeignTable(span())"
        "SqlAlterUser(span())"
        "SqlReassignOwned(span())"
        "SqlRefreshForeignTables(span())"
        "SqlRenameTable(span())"
        "SqlInsertIntoTable(span())"
        "SqlKillQuery(span())"
        "SqlCustomDrop(span())"
        "SqlCustomShow(span())"
        "SqlGrant(span())"
        "SqlRevoke(span())"
        "SqlDumpTable(span())"
        "SqlRestoreTable(span())"
        "SqlTruncateTable(span())"
        "SqlOptimizeTable(span())"
        "SqlCopyTable(span())"
        "SqlValidateSystem(span())"
        "SqlAlterSystemClear(span())"
        "SqlAlterSessionSet(span())"
